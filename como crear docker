docker push obryanvip/androidstudio:1.5 



#con clave docker run -d --name YourName --shm-size 2G -P -p 8888:5901 -p 8899:6901 -e VNC_RESOLUTION=1440x900 -e VNC_PW=ppllmmoo --device /dev/kvm --user $(id -u):$(id -g) obryanvip/androidstudio:2023
#sin clave  docker run -d --name obry --shm-size 2G -P -p 8888:5901 -p 8899:6901 -e VNC_RESOLUTION=1440x900  --device /dev/kvm --user $(id -u):$(id -g) obryanvip/androidstudio:2023


